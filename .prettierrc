{
  "singleQuote": true,
  "semi": true,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^react$",
    "^next/(.*)$",
    "",

    "<THIRD_PARTY_MODULES>",
    "",

    "^@/db$",
    "^@/db/(.*)$",
    "",

    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "",

    "^@/hooks/(.*)$",
    "",

    "^@/lib/(.*)$",
    "",

    "^[./]"
  ],
  "importOrderTypeScriptVersion": "5.0.0"
}
